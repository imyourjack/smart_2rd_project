<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <properties resource="mybatis/db.properties"/>
  <typeAliases>
      <typeAlias type="model.UserVO" alias="userVO"/>
      <typeAlias type="model.wonduVO" alias="wonduVO"/>
      <typeAlias type="model.BoardVO" alias="boardVO"/> 
      <typeAlias type="model.wonduCVO" alias="wonduCVO"/> 
      <typeAlias type="model.GoodVO" alias="goodVO"/> 
  </typeAliases> 
  <environments default="development">
    <environment id="development">
      <transactionManager type="JDBC"/>
      <dataSource type="POOLED">
        <property name="driver" value="${driver}"/>
        <property name="url" value="${url}"/>
        <property name="username" value="${username}"/>
        <property name="password" value="${password}"/>                 
      </dataSource>
    </environment>
  </environments>
  <mappers>
    <mapper resource="mybatis/BoardMapper.xml"/>
  </mappers>
</configuration>